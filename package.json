{
  "name": "wtttf",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev:ws": "ts-node-dev --project tsconfig.server.json --respawn --transpile-only src/server/wsServer.ts",
    "dev": "run-p dev:*",
    "build:1-generate": "prisma generate",
    "build:2-migrate": "prisma migrate deploy",
    "build:3-next": "cross-env NODE_ENV=production next build",
    "build:4-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "start": "cross-env NODE_ENV=production node dist/src/server/prodServer.js",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true,
    "semiColon": true
  },
  "dependencies": {
    "@heroicons/react": "^2.0.10",
    "@hookform/resolvers": "^2.9.8",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.3.1",
    "@trpc/client": "^9.27.2",
    "@trpc/next": "^9.27.2",
    "@trpc/react": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "daisyui": "^2.25.0",
    "ioredis": "^5.2.3",
    "next": "12.2.5",
    "next-auth": "^4.10.3",
    "node-fetch": "^2.6.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.35.0",
    "react-query": "^3.39.2",
    "sass": "^1.54.9",
    "sharp": "^0.31.0",
    "superjson": "^1.9.1",
    "ws": "^8.8.1",
    "zod": "^3.19.1",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-next": "12.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4"
  },
  "ct3aMetadata": {
    "initVersion": "5.10.1"
  }
}
